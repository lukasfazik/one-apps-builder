CONTEXT = [
  CI_SERVER_TOKEN = "$CI_SERVER_TOKEN",
  CI_SERVER_URL = "$CI_SERVER_URL",
  CRYPTED_PASSWORD = "$USER[CRYPTED_PASSWORD]",
  DEV_PREFIX = "sd",
  EJECT_CDROM = "YES",
  NETWORK = "YES",
  PASSWORD = "$USER[PASSWORD]",
  SSH_PUBLIC_KEY = "$USER[SSH_PUBLIC_KEY]",
  START_SCRIPT_BASE64 = "",
  USERNAME = "$USER[USERNAME]" ]
CPU = "0.2"
CPU_MODEL = [
  FEATURES = "svm",
  MODEL = "EPYC-IBPB" ]
DISK = [
  DEV_PREFIX = "sd",
  DISCARD = "unmap",
  IMAGE = "Debian 12.0.0",
  IMAGE_UNAME = "oneadmin" ]
DISK = [
  CACHE = "unsafe",
  DEV_PREFIX = "sd",
  DISCARD = "unmap",
  FORMAT = "raw",
  SIZE = "2048",
  TYPE = "swap" ]
FEATURES = [
  ACPI = "yes",
  APIC = "yes",
  GUEST_AGENT = "yes",
  HYPERV = "no",
  LOCALTIME = "yes",
  PAE = "yes",
  VIRTIO_BLK_QUEUES = "auto",
  VIRTIO_SCSI_QUEUES = "auto" ]
GRAPHICS = [
  LISTEN = "0.0.0.0",
  TYPE = "VNC" ]
HOT_RESIZE = [
  CPU_HOT_ADD_ENABLED = "YES",
  MEMORY_HOT_ADD_ENABLED = "YES" ]
LOGO = "images/logos/debian.png"
MEMORY = "16384"
MEMORY_MAX = "65536"
MEMORY_RESIZE_MODE = "BALLOONING"
MEMORY_UNIT_COST = "MB"
NIC = [
  NETWORK = "503-usrpriv",
  NETWORK_UNAME = "oneadmin",
  SECURITY_GROUPS = "0",
  SSH = "YES" ]
OS = [
  ARCH = "x86_64",
  BOOT = "disk0",
  FIRMWARE = "BIOS",
  MACHINE = "q35" ]
USER_INPUTS = [
  CI_SERVER_TOKEN = "M|password|Gitlab runner token",
  CI_SERVER_URL = "M|text|Gitlab CI server url| |https://gitlab.fi.muni.cz" ]
VCPU = "16"